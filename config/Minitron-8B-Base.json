{
    "type": "function",
    "tenant": "t1",
    "namespace": "ns1",
    "name": "Minitron-8B-Base",
    "spec": {
        "image": "vllm/vllm-openai:v0.8.4",
        "commands": [
            "--model",
            "nvidia/Minitron-8B-Base",
            "--enforce-eager",
            "--disable-custom-all-reduce",
            "--trust-remote-code",
            "--max-model-len",
            "2000",
            "--tensor-parallel-size=2"
        ],
        "resources": {
            "CPU": 6000,
            "Mem": 50000,
            "GPU": {
                "Type": "Any",
                "Count": 2,
                "vRam": 13800
            }
        },
        "envs": [
            [
                "LD_LIBRARY_PATH",
                "/Quark/target/debug/:$LD_LIBRARY_PATH"
            ]
        ],
        "mounts": [
            {
                "hostpath": "/home/brad/cache",
                "mountpath": "/root/.cache/huggingface"
            }
        ],
        "endpoint": {
            "path": "/v1/completions",
            "port": 8000,
            "schema": "Http"
        },
        "probe": {
            "path": "/health",
            "port": 8000,
            "schema": "Http"
        },
        "api_type": {
            "openai": {
                "name": "nvidia/Minitron-8B-Base",
                "max_tokens": 1000,
                "temperature": 0
            }
        },
        "keepalive": "Blob"
    }
}