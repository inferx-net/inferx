{
    "type": "function",
    "tenant": "public",
    "namespace": "deepseek-ai",
    "name": "DeepSeek-R1-Distill-Llama-8B",
    "object": {
        "spec": {
            "image": "vllm/vllm-openai:v0.10.0",
            "commands": [
                "--model",
                "/root/.cache/huggingface/git/DeepSeek-R1-Distill-Llama-8B",
                "--served-model-name",
                "deepseek-ai/DeepSeek-R1-Distill-Llama-8B",
                "--uvicorn-log-level",
                "warning",
                "--async-scheduling",
                "--trust-remote-code",
                "--max-model-len",
                "2000",
                "--tensor-parallel-size=2"
            ],
            "resources": {
                "CPU": 20000,
                "Mem": 60000,
                "GPU": {
                    "Type": "Any",
                    "Count": 2,
                    "vRam": 13800
                }
            },
            "envs": [
                [
                    "LD_LIBRARY_PATH",
                    "/usr/local/lib/python3.12/dist-packages/nvidia/cuda_nvrtc/lib/:$LD_LIBRARY_PATH"
                ],
                [
                    "TORCH_CUDA_ARCH_LIST",
                    "8.6"
                ]
            ],
            "mounts": [
                {
                    "hostpath": "/opt/inferx/cache",
                    "mountpath": "/root/.cache/huggingface"
                }
            ],
            "endpoint": {
                "port": 8000,
                "schema": "Http",
                "probe": "/health"
            },
            "sample_query": {
                "apiType": "text2text",
                "prompt": "what is the integral of x^2 from 0 to 2?\nPlease reason step by step, and put your final answer within \\boxed{}.",
                "path": "v1/completions",
                "body": {
                    "model": "deepseek-ai/DeepSeek-R1-Distill-Llama-8B",
                    "max_tokens": "1000",
                    "temperature": "0",
                    "stream": "true"
                }
            },
            "standby": {
                "gpu": "Blob",
                "pageable": "Blob",
                "pinned": "Blob"
            },
            "policy": {
                "Obj": {
                    "min_replica": 0,
                    "max_replica": 1,
                    "standby_per_node": 1,
                    "parallel": 200,
                    "runtime_config": {
                        "graph_sync": false
                    }
                }
            }
        }
    }
}