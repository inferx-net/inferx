{
    "type": "function",
    "tenant": "public",
    "namespace": "Qwen",
    "name": "Hermes-2-Pro-Llama-3-8B-AWQ",
    "object": {
        "spec": {
            "image": "vllm/vllm-openai:v0.6.4.post1",
            "commands": [
                "--model",
                "solidrust/Hermes-2-Pro-Llama-3-8B-AWQ",
                "--disable-custom-all-reduce",
                "--quantization",
                "awq",
                "--max-model-len",
                "8192",
                "--uvicorn-log-level",
                "warning",
                "--enable-chunked-prefill",
                "--enable-prefix-caching"
            ],
            "resources": {
                "CPU": 12000,
                "Mem": 24000,
                "ReadyMem": 3000,
                "GPU": {
                    "Type": "Any",
                    "Count": 1,
                    "vRam": 13000
                }
            },
            "envs": [
                [
                    "LD_LIBRARY_PATH",
                    "/usr/local/lib/python3.12/dist-packages/nvidia/cuda_nvrtc/lib/:$LD_LIBRARY_PATH"
                ],
                [
                    "VLLM_CUDART_SO_PATH",
                    "/usr/local/cuda-12.1/targets/x86_64-linux/lib/libcudart.so.12"
                ],
                [
                    "UVICORN_ACCESS_LOG",
                    "0"
                ]
            ],
            "mounts": [
                {
                    "hostpath": "/opt/inferx/cache",
                    "mountpath": "/root/.cache/huggingface"
                }
            ],
            "endpoint": {
                "port": 8000,
                "schema": "Http",
                "probe": "/health"
            },
            "sample_query": {
                "apiType": "text2text",
                "prompt": "Can you provide ways to eat combinations of bananas and dragonfruits?",
                "path": "v1/completions",
                "body": {
                    "model": "solidrust/Hermes-2-Pro-Llama-3-8B-AWQ",
                    "max_tokens": "8000",
                    "temperature": "0",
                    "stream": "true"
                }
            },
            "standby": {
                "gpu": "Mem",
                "pageable": "File",
                "pinned": "Mem"
            },
            "policy": {
                "Link": {
                    "objType": "funcpolicy",
                    "namespace": "system",
                    "name": "default_funcpolicy"
                }
            }
        }
    }
}