{
    "type": "function",
    "tenant": "t1",
    "namespace": "ns1",
    "name": "mistral",
    "spec": {
        "image": "vllm/vllm-openai:v0.6.2",
        "commands": [
            "--model",
            "mistralai/Mistral-7B-v0.1",
            "--enforce-eager"
        ],
        "resources": {
            "CPU": 100,
            "Mem": 200,
            "GPU": {
                "Type": "RTX3060",
                "Count": 2
            }
        },
        "envs": [
            [
                "LD_LIBRARY_PATH",
                "/usr/local/lib/python3.12/dist-packages/nvidia/cuda_nvrtc/lib/:$LD_LIBRARY_PATH"
            ]
        ],
        "mounts": [
            {
                "hostpath": "/home/brad/cache",
                "mountpath": "/root/.cache/huggingface"
            }
        ],
        "endpoint": {
            "path": "/v1/completions",
            "port": 8000,
            "schema": "Http"
        },
        "probe": {
            "path": "/health",
            "port": 8000,
            "schema": "Http"
        },
        "api_type": {
            "openai": {
                "name": "mistralai/Mistral-7B-v0.1",
                "max_tokens": 200,
                "temperature": 0
            }
        }
    }
}